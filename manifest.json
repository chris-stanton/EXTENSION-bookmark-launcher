
{
  "manifest_version": 2,

  "name": "Bookmark Quick Launcher",
  "description": "Bookmark quick launcher (Chris Stanton's bookmarks)",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "extension.html"
  },
  "permissions": ["activeTab"]
}
